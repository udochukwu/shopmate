{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier", "prettier/react", "eslint:recommended"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": 0,
    "jsx-a11y/aria-role": 0
  }
}
